   "source": [
    "import cv2\n",
    "\n",
    "cam = cv2.VideoCapture(0)\n",
    "\n",
    "while True:\n",
    "\tret, image = cam.read()\n",
    "\tcv2.imshow('Imagetest',image)\n",
    "\tk = cv2.waitKey(1)\n",
    "\tif k != -1:\n",
    "\t\tbreak\n",
    "cv2.imwrite('/home/pi/testimage.jpg', image)\n",
    "cam.release()\n",
    "cv2.destroyAllWindows()"
   ]
  }
